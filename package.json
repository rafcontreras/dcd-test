{
  "name": "dcd-test",
  "version": "1.0.0",
  "description": "Test for David Clark Designs",
  "main": "index.js",
  "repository": "git@github.com:rafcontreras/dcd-test.git",
  "author": "Rafael Contreras <raf@contrer.as>",
  "license": "CC0",
  "private": true,
  "scripts": {
    "api-server:build": "webpack --config webpack.prod.config.js",
    "api-server:develop": "webpack --config webpack.dev.config.js --watch",
    "build": "yarn env:prod && yarn docs:build && yarn gatsby:build && yarn api-server:build",
    "coverage": "jest --coverage",
    "develop": "concurrently \"yarn api-server:develop\" \"yarn gatsby:develop\"",
    "docs:build": "node ./expressSrc/scripts/createDocs.js",
    "env:prod": "cross-env NODE_ENV=production && cross-env CI=true && cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "gatsby:build": "yarn env:prod && gatsby build --log-pages --no-uglify --no-colors",
    "gatsby:clean": "gatsby clean",
    "gatsby:develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "postinstall": "patch-package",
    "pretest": "cross-env NODE_ENV=test",
    "start:production": "cross-env NODE_ENV=production && node ./dist/main.js",
    "start": "yarn build && yarn start:production",
    "test": "jest"
  },
  "dependencies": {
